- model_class = Execution
.page-header
  %h1=t '.title', :default => model_class.model_name.human.titleize

%p
  %strong= model_class.human_attribute_name(:executed_at) + ':'
  %br
  = @execution.executed_at
%p
  %strong= model_class.human_attribute_name(:executor_id) + ':'
  %br
  = @execution.executor_id


= link_to t('.back', :default => t("helpers.links.back")), executions_path, :class => 'btn btn-default'
= link_to t('.edit', :default => t("helpers.links.edit")), edit_execution_path(@execution), :class => 'btn btn-default'
= link_to t('.destroy', :default => t("helpers.links.destroy")), execution_path(@execution), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
